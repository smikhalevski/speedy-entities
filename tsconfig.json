{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES5",
    "module": "ES6",
    "rootDir": "./src",
    "outDir": "./lib",
    "lib": [
      "ES7"
    ]
  },
  "include": [
    "./src/**/*"
  ]
}
